<?xml version="1.0" encoding="utf-8"?>

                                                              
                                                        
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Basic.SignUp"
    android:background="@drawable/blur_background" android:tag="layout/activity_sign_up_0" xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SignUp"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.062" />

    <EditText
        android:id="@+id/sign_up_First_Name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/roundcorners"
        android:hint="Name"
        android:paddingLeft="9dp"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:textColorHint="#090909"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_Last_Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/sign_up_Last_Name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:background="@drawable/roundcorners"
        android:hint="Surname"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:paddingLeft="9dp"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_email_id"
        tools:layout_editor_absoluteX="8dp" />

    <EditText
        android:id="@+id/sign_up_email_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:paddingLeft="9dp"
        android:hint=" Email Id"
        android:background="@drawable/roundcorners"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_Phone_No"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/sign_up_Phone_No"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:paddingLeft="9dp"
        android:hint=" Phone No."
        android:background="@drawable/roundcorners"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/Sign_Up_password"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/Sign_Up_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:paddingLeft="9dp"
        android:hint=" Password"
        android:password="true"
        android:background="@drawable/roundcorners"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_renter_password"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/sign_up_renter_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="140dp"
        android:background="@drawable/roundcorners"
        android:hint="Confirm Password"
        android:paddingLeft="9dp"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:password="true"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="8dp" />

    <ImageView
        android:layout_width="417dp"
        android:layout_height="147dp"
        android:background="@drawable/applogo11"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.265"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-3dp" />

    <Button
        android:id="@+id/Sign_Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign Up"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.954" />


</androidx.constraintlayout.widget.ConstraintLayout>

         