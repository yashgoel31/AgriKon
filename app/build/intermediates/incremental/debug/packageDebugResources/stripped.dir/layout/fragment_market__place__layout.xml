<?xml version="1.0" encoding="utf-8"?>
                                                            
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Farmer_Market.MarketPlace.Market_Place_Layout"
    android:background="@drawable/blur_background" android:tag="layout/fragment_market__place__layout_0" xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Market Type"
        android:id="@+id/Market_Id"
        android:visibility="invisible"/>


    <androidx.recyclerview.widget.RecyclerView
